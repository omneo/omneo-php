{
  "name": "omneo/omneo-php",
  "description": "PHP library for the Omneo API",
  "type": "library",
  "license": "MIT",
  "require": {
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/support": "^5.6",
    "nesbot/carbon": "^2.0",
    "justinrainbow/json-schema": "^5.2",
    "symfony/psr-http-message-bridge": "^1.0",
    "zendframework/zend-diactoros": "^1.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Omneo\\" : "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omneo\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Omneo\\Laravel\\ServiceProvider"
      ]
    }
  }
}
